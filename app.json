{
  "name": "catbox-uploader-bot",
  "description":"You can use this bot upload images, videos and animations to the catbox link.",
  "repository": "https://github.com/SendiAp/catbox-uploader-bot",
  "logo": "https://files.catbox.moe/nou8eo.jpg",
  "keywords": [
    "images",
    "videos",
    "animation",
    "uploader"
  ],
      "env": {
        "API_ID": {
            "description": "Get from my.telegram.org/apps",
            "required": true
        },
        "API_HASH": {
            "description": "Get from my.telegram.org/apps",
            "required": true
        },
        "BOT_TOKEN": {
            "description": "Get from Telegram's @BotFather",
            "required": true
        }
    },
    "formation": {
    "worker": {
      "quantity": 1,
      "size": "free"
    }
  },
    "buildpacks": [
        {
            "url": "heroku/python"
        }
    ]
}
